{
  "name": "vite-express-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "vite",
    "dev:server": "tsx watch src/server/main.ts",
    "start": "NODE_ENV=production node dist/server/main.js",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "vite build",
    "build:backend": "tsc --project src/server/tsconfig.json && cp -r src/server/templates dist/server/templates",
    "format": "prettier --write .",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.18.0",
    "@google/genai": "^1.0.0",
    "@maxmind/geoip2-node": "^6.1.0",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@segment/analytics-node": "^2.3.0",
    "@sentry/node": "^9.33.0",
    "@sentry/react": "^9.33.0",
    "@tailwindcss/vite": "^4.1.10",
    "@types/multer": "^2.0.0",
    "@types/react-select": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "express-session": "^1.18.2",
    "http-proxy-middleware": "^3.0.5",
    "lightningcss": "^1.30.1",
    "lucide-react": "^0.522.0",
    "multer": "^2.0.2",
    "nanoid": "^5.1.5",
    "portkey-ai": "^3.0.1",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^6.30.1",
    "react-select": "^5.10.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.10",
    "tsx": "^4.19.3",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5.8.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@playwright/test": "^1.53.2",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.2",
    "@types/node": "^22.15.2",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.2.0",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "nodemon": "^3.1.10",
    "playwright": "^1.53.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.3"
  }
}

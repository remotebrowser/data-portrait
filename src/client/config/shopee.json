{
  "brand_id": "shopee",
  "brand_name": "Shopee",
  "logo_url": "https://i.pinimg.com/736x/89/f3/62/89f362b04423b091aee923a0387c5c35.jpg",
  "is_mandatory": false,
  "is_dpage": true,
  "schema": [
    {
      "name": "loginKey",
      "type": "text",
      "prompt": "No. Handphone/Username/Email"
    },
    {
      "name": "password",
      "type": "password",
      "prompt": "Password"
    },
    {
      "name": "submit",
      "type": "click",
      "prompt": "Sign In",
      "dependsOnFields": "email"
    }
  ],
  "dataTransform": {
    "dataPath": "",
    "fieldMappings": [
      {
        "outputKey": "brand",
        "sourcePath": "brand",
        "defaultValue": "Shopee"
      },
      {
        "outputKey": "order_date",
        "sourcePath": "order_date",
        "defaultValue": "N/A",
        "transform": "date"
      },
      {
        "outputKey": "order_total",
        "sourcePath": "total_price",
        "defaultValue": "N/A"
      },
      {
        "outputKey": "order_id",
        "sourcePath": "store_nae",
        "defaultValue": "N/A"
      },
      {
        "outputKey": "product_names",
        "sourcePath": "product_name",
        "defaultValue": "N/A",
        "convertToArray": true
      },
      {
        "outputKey": "image_urls",
        "sourcePath": "product_image",
        "defaultValue": "N/A",
        "transform": "image",
        "convertToArray": true
      }
    ]
  }
}
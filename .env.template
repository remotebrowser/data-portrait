# GetGather API Config
GETGATHER_URL=

MAXMIND_ACCOUNT_ID=
MAXMIND_LICENSE_KEY=

# Image Generation Providers (choose one or both)
# Provider selection priority: PORTKEY_API_KEY > GEMINI_API_KEY
# - If PORTKEY_API_KEY is set, uses Portkey
# - If only GEMINI_API_KEY is set, uses Google GenAI
# - If both are set, Portkey is prioritized
PORTKEY_API_KEY=
GEMINI_API_KEY=

# DeepInfra API (optional)
# Used for background blur when "Background Blur" trait is selected
DEEPINFRA_API_KEY=

SENTRY_DSN=

# Google Cloud Storage (optional)
# If not configured, images will be stored locally in public/ directory
GCS_BUCKET_NAME=
GCS_PROJECT_ID=

# Google Cloud Auth (choose one):
# - Local dev: Use GOOGLE_APPLICATION_CREDENTIALS with file path
# - Production: Use GOOGLE_APPLICATION_CREDENTIALS_JSON with base64-encoded service account JSON
#   To create base64: base64 -w 0 path/to/service-account.json
GOOGLE_APPLICATION_CREDENTIALS=
GOOGLE_APPLICATION_CREDENTIALS_JSON=

APP_HOST=http://localhost:3000

